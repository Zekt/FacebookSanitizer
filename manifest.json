{
	"manifest_version": 2,

	"name": "Facebook Sanitizer",
	"description": "This extension sanitizes your Facebook.",
	"version": "0.1",

	"browser_action": {
		"default_title": "Long Live Sugar Cubs!!"
	},
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
			"js": ["content.js"]
		}
	],
	"web_accessible_resources": [
		"stickerURLs/*"
	]
}
